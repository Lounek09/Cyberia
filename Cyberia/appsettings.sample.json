{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/log-.txt",
          "rollingInterval": "Month"
        }
      }
    ]
  },
  "Cyberia": {
    "EnableCheckCytrus": false,
    "CheckCytrusInterval": 1, //in minutes
    "EnableCheckLang": false,
    "CheckLangInterval": 5, //in minutes
    "EnableCheckBetaLang": false,
    "CheckBetaLangInterval": 5, //in minutes
    "EnableCheckTemporisLang": false,
    "CheckTemporisLangInterval": 5, //in minutes
    "BotConfig": {
      "Token": "",
      "EmbedColor": "#FFFFFF",
      "DiscordServerInviteUrl": "",
      "LogChannelId": 0,
      "CommandErrorChannelId": 0,
      "LangForumChannelId": 0,
      "CytrusChannelId": 0,
      "CytrusManifestChannelId": 0,
      "ApiConfig": {
        "CdnUrl": "",
        "Temporis": false
      }
    }
  }
}
